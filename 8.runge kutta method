import numpy as np
from scipy.integrate import solve_ivp

def func(x, y):
    return x + y

x0, y0 = 0, 1
x_end = 1.1

solution = solve_ivp(func, (x0, x_end), [y0], method='RK45', t_eval=np.linspace(x0, x_end, int((x_end-x0)/0.1)+1))

y_1_1 = solution.y[0][-1]

print(f"y(1.1) â‰ˆ {y_1_1:.6f}")
